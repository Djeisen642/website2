{
  "name": "website2",
  "version": "1.0.0",
  "description": "A website for Jason S.",
  "main": "index.js",
  "repository": "git@github.com:Djeisen642/website2.git",
  "author": "Jason <jasonssuttles@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@4.4.0",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint:js": "echo \"ESLINT --\" && eslint .",
    "lint:style": "echo \"STYLELINT --\" && stylelint \"**/*.{vue,scss,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style && yarn check:types",
    "check:types": "echo \"TSC --\" && tsc --noEmit",
    "postinstall": "nuxt prepare && husky install",
    "postpack": "pinst --enable",
    "prepack": "pinst --disable",
    "preview": "nuxt preview",
    "start": "yarn dev",
    "up-i": "yarn upgrade-interactive && yarn up -R",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@nuxt/eslint": "^0.5.0",
    "@nuxtjs/device": "^3.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/eslint": "^9",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^22.2.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.9.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "firebase-tools": "^13.17.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "node-fetch": "^3.3.2",
    "nuxt": "^3.12.4",
    "pinst": "^3.0.0",
    "postcss-html": "^1.7.0",
    "sass": "^1.77.8",
    "stylelint": "^16.8.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-plugin-vuetify": "^2.0.4"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@nuxtjs/robots": "^4.0.2",
    "@pinia/nuxt": "^0.5.3",
    "@vueuse/core": "^10.11.1",
    "@vueuse/nuxt": "^10.11.1",
    "firebase": "^10.12.5",
    "prettier": "^3.3.3",
    "uuid": "^10.0.0",
    "vue": "^3.4.37",
    "vuetify": "3.6.14",
    "web-vitals": "^4.2.3"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": "please-use-npm",
    "node": "lts/iron"
  },
  "volta": {
    "node": "20.16.0",
    "yarn": "4.4.0"
  }
