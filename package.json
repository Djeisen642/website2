    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "firebase-tools": "^11.24.0",
    "husky": "^8.0.3",
    "nuxt": "3.0.x",
    "pinst": "^3.0.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.4",
