  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint:js": "echo \"ESLINT --\" && eslint .",
    "lint:style": "echo \"STYLELINT --\" && stylelint \"**/*.{vue,scss,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style && yarn check:types && yarn nuxt:types",
    "check:types": "echo \"TSC --\" && tsgo --noEmit",
    "nuxt:types": "nuxt typecheck",
    "postinstall": "nuxt prepare && husky install",
    "postpack": "pinst --enable",
    "prepack": "pinst --disable",
    "preview": "nuxt preview",
    "start": "yarn dev",
    "up-i": "yarn upgrade-interactive && yarn up -R",
    "prepare": "husky"
  },
