{
  "name": "website2",
  "version": "1.0.0",
  "description": "A website for Jason S.",
  "main": "index.js",
  "repository": "git@github.com:Djeisen642/website2.git",
  "author": "Jason <jasonssuttles@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint:js": "echo \"ESLINT --\" && eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:style": "echo \"STYLELINT --\" && stylelint \"**/*.{vue,scss,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style && yarn check:types",
    "check:types": "echo \"TSC --\" && tsc --noEmit",
    "postinstall": "nuxt prepare && husky install",
    "postpack": "pinst --enable",
    "prepack": "pinst --disable",
    "preview": "nuxt preview",
    "start": "yarn dev",
    "up-i": "yarn upgrade-interactive && yarn up -R",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@nuxtjs/device": "^3.1.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@rushstack/eslint-patch": "^1.7.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.11.19",
    "@types/uuid": "^9.0.8",
    "axe-core": "^4.8.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vuejs-accessibility": "^2.2.1",
    "firebase-tools": "^13.3.0",
    "husky": "^9.0.11",
    "node-fetch": "^3.3.2",
    "nuxt": "^3.10.2",
    "pinst": "^3.0.0",
    "postcss-html": "^1.6.0",
    "sass": "^1.71.0",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-vuetify": "^2.0.1",
    "volar-service-prettier": "^0.0.30",
    "vue-axe": "^3.1.2"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@nuxtjs/robots": "^3.0.0",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/core": "^10.7.2",
    "@vueuse/nuxt": "^10.7.2",
    "firebase": "^10.8.0",
    "prettier": "^3.2.5",
    "uuid": "^9.0.1",
    "vue": "^3.4.19",
    "vuetify": "3.5.4",
    "web-vitals": "^3.5.2"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": "please-use-npm",
    "node": "lts/iron"
  },
  "volta": {
    "node": "20.10.0",
    "yarn": "4.0.2"
  }
}
