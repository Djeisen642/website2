  "author": "Jason <jasonssuttles@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@4.7.0",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint:js": "echo \"ESLINT --\" && eslint .",
    "lint:style": "echo \"STYLELINT --\" && stylelint \"**/*.{vue,scss,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style && yarn check:types",
    "check:types": "echo \"TSC --\" && tsc --noEmit",
    "postinstall": "nuxt prepare && husky install",
    "postpack": "pinst --enable",
    "prepack": "pinst --disable",
    "preview": "nuxt preview",
    "start": "yarn dev",
    "up-i": "yarn upgrade-interactive && yarn up -R",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@firebase/app": "^0.11.3",
    "@nuxt/eslint": "^1.2.0",
    "@nuxtjs/device": "^3.2.4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/eslint": "^9.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^22.13.11",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.23.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "firebase-tools": "^14.1.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "node-fetch": "^3.3.2",
    "nuxt": "^3.16.1",
    "pinia": "^3.0.1",
    "pinst": "^3.0.0",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "sass-embedded": "^1.86.0",
    "stylelint": "^16.16.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.6",
    "vite-plugin-vuetify": "^2.1.0"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@nuxtjs/robots": "^5.2.8",
